{
  "presets": ["env", "react", "stage-0", "stage-1"],
  "sourceMaps": true,
  "retainLines": true,
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["babel-plugin-root-import", {
      "rootPathSuffix": ""
    }]

  ],
  "env": {
    "production": {
      "plugins": [
        ["babel-plugin-root-import", {
          "rootPathSuffix": ""
        }]
      ]
    }
  }
}